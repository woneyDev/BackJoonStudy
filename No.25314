import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc              = new Scanner(System.in);
        int N                   = sc.nextInt();
        int size                = N/4;
        StringBuilder result    = new StringBuilder("");

        if ( N % 4 == 0 ) {

            for ( int i = 0; i < size; i ++ ) {

                result.append("long ");

            }
            result.append("int");
        }
        System.out.println(result);
    }
}
