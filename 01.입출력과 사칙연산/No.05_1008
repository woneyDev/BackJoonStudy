import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double A = 0;
        double B = 0;
        A = sc.nextInt();
        B = sc.nextInt();

        if ( A > 0 && B < 10 ) {
            double C = (A/B);
            System.out.println(C);
        }
    }
}
